import threading
import time

lock1 = threading.Lock()
lock2 = threading.Lock()

def task1():
    with lock1:
        t = "Treasure full of gold"
        f = 50
        print("In lock1 this place is containing")
        time.sleep(1)
        with lock2:
            print("Om Saini is the owner")

def task2():
    with lock1:                      
        print("Treasure full of gold")
        time.sleep(1)
        with lock2:
            print("Pure fool guy I have ever seen")

t1 = threading.Thread(target=task1)
t2 = threading.Thread(target=task2)

t1.start()
t2.start()

t1.join()
t2.join()
